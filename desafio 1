    public static void main(String[] args) {
        Animal cachorro = new Cachorro("Rex", 5);
        Animal gato = new Gato("Mia", 3);
        Animal passaro = new Passaro("Piu", 1);

        cachorro.imprimirInformacoes();
        gato.imprimirInformacoes();
        passaro.im
